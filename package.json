{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f .env.local npx ts-node ./src/index",
    "dev:nodemon": "env-cmd -f .env.local npx nodemon",
    "build": "rimraf ./build && npx tsc",
    "start": "npm run build && ts-node -r tsconfig-paths/register ./build/index",
    "lint": "npx eslint ./src",
    "lint-fix": "npx eslint ./src --fix",
    "pretty": "prettier --write ."
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
